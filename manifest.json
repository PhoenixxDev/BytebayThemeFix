{
    "manifest_version": 2,
    "name": "Bytebay Theme Modifier",
    "version": "1.0",
    "permissions": ["activeTab"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["https://bytebay.cc/*"],
        "js": ["content.js"]
      }
    ]
  }